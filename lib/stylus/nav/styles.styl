.NavigationContainer
    line-height 60px
    margin 0
    margin-right 1rem
    padding 0
    width 40%
    z-index 5
    &-list
        width 80%
        display flex
        justify-content space-between
        margin 0 auto
        padding 0
    &-item
        text-decoration none
        list-style-type none
    &-link
        font-family 'Titillium Web'
        font-size 1.2rem
        text-decoration none
        color primaryFontDarkColor
